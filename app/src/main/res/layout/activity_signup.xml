<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signup">

    <RelativeLayout
        android:id="@+id/halaman_awa"
        android:layout_width="417dp"
        android:layout_height="737dp"
        android:background="#FFC736"
        android:clipToOutline="true"
        tools:ignore="MissingConstraints" >

        <TextView
            android:id="@+id/recetasdecomida"
            android:layout_width="352dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="656dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/bebas_neue"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-21sp"
            android:text="Recetas De Comida"
            android:textColor="#134096"
            android:textSize="50dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="317dp"
            android:layout_height="244dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="51dp"
            android:layout_marginEnd="51dp"
            android:layout_marginBottom="413dp"
            app:srcCompat="@drawable/gambarsignup" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="349dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="335dp"
            android:fontFamily="@font/poppins_bold"
            android:text="-DAFTAR AKUN APLIKASI-"
            android:textAlignment="center"
            android:textColor="#134096"
            android:textSize="27dp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="73dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            android:layout_marginBottom="257dp"
            app:srcCompat="@drawable/kotakputih" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            android:layout_marginBottom="171dp"
            app:srcCompat="@drawable/kotakputih" />
        <Button
            android:id="@+id/buttonsignup"
            android:layout_width="261dp"
            android:layout_height="65dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="82dp"
            android:layout_marginBottom="101dp"
            android:text="Daftar Sekarang"
            android:textSize="17dp"
            android:textColor="#FFC736"
            app:iconTint="#134096"
            app:rippleColor="#134096"
            app:strokeColor="#134096" />

        <Button
            android:id="@+id/buttonkelogin"
            android:layout_width="113dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="219dp"
            android:layout_marginEnd="85dp"
            android:layout_marginBottom="64dp"
            android:text="Login Disini"
            android:textColor="#FFC736"
            android:textSize="10dp"
            app:iconTint="#134096"
            app:rippleColor="#134096"
            app:strokeColor="#134096"
            tools:ignore="TextSizeCheck"
            android:onClick="pindahkelogin"/>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="70dp"
            android:layout_marginEnd="197dp"
            android:layout_marginBottom="71dp"
            android:fontFamily="@font/poppins_semibold"
            android:text=" Sudah Punya Akun? "
            android:textAlignment="center"
            android:textColor="#134096"
            android:textSize="13dp"
            tools:ignore="TextSizeCheck" />

        <EditText
            android:id="@+id/emailsignup"
            android:layout_width="309dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="48dp"
            android:layout_marginEnd="60dp"
            android:layout_marginBottom="270dp"
            android:ems="10"
            android:fontFamily="@font/poppins_semibold"
            android:hint="Email"
            android:inputType="textEmailAddress"
            android:textColor="#134096"
            android:textColorHighlight="#134096"
            android:textColorHint="#134096"
            />

        <EditText
            android:id="@+id/passwordsignup"
            android:layout_width="306dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="48dp"
            android:layout_marginEnd="63dp"
            android:layout_marginBottom="184dp"
            android:ems="10"
            android:fontFamily="@font/poppins_semibold"
            android:hint="Password"
            android:inputType="textPassword"
            android:textColor="#134096"
            android:textColorHighlight="#134096"
            android:textColorHint="#134096"
            />


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>