<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Fragment.AccountFragment">

    <RelativeLayout
        android:id="@+id/halaman_awal"
        android:layout_width="417dp"
        android:layout_height="737dp"
        android:background="#FFC736"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,UselessParent">


        <TextView
            android:id="@+id/myaccount3"
            android:layout_width="417dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView13"
            android:layout_alignStart="@+id/imageView13"
            android:layout_alignEnd="@+id/imageView13"
            android:layout_marginStart="-114dp"
            android:layout_marginTop="43dp"
            android:layout_marginEnd="-99dp"
            android:layout_marginBottom="30dp"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-21sp"
            android:text="Selamat Datang dan Selamat Memasak"
            android:textColor="#134096"
            android:textSize="19sp" />

        <TextView
            android:id="@+id/emailuser"
            android:layout_width="417dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView13"
            android:layout_alignStart="@+id/imageView13"
            android:layout_alignEnd="@+id/imageView13"
            android:layout_marginStart="-116dp"
            android:layout_marginTop="86dp"
            android:layout_marginEnd="-96dp"
            android:layout_marginBottom="30dp"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-21sp"
            android:text="email@gmail.com"
            android:textColor="#134096"
            android:textSize="19sp" />

        <TextView
            android:id="@+id/myaccount2"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="89dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="30dp"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-21sp"
            android:text="MY ACCOUNT"
            android:textColor="#134096"
            android:textSize="30sp" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="204dp"
            android:layout_height="184dp"
            android:layout_below="@+id/myaccount2"
            android:layout_alignStart="@+id/myaccount2"
            android:layout_alignEnd="@+id/myaccount2"
            android:layout_marginStart="81dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="75dp"
            android:src="@drawable/baseline_account_circle_24"
            app:tint="@color/my_blue" />

        <Button
            android:id="@+id/buttonlogout"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="113dp"
            android:layout_marginEnd="112dp"
            android:layout_marginBottom="184dp"
            android:fontFamily="@font/bebas_neue"
            android:text="LOGOUT"
            android:textSize="30sp" />


    </RelativeLayout>

</FrameLayout>